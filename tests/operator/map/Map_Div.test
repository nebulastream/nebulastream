# name: map/Map_Div.test
# description: Simple map tests with a div function
# groups: [Map]

# LLVM LIT configuration
# RUN: sed -e "s@SINK@%sink@g" -e "s@TESTDATA@%testdata@g"< %s > %t &&  rm %tsink || true
# RUN: %client test -f %t -s %worker_address && %result_checker %t %tsink

Source window INT64 id INT64 value INT64 timestamp
5,1,1000
10,1,1001
15,1,1002
20,2,2000
25,19,19000
30,20,20000
35,21,21000

# Map with div function touches one field
Query::from("window")
    .map(Attribute("id") = Attribute("id") / 5)
    .SINK;
----
1,1,1000
2,1,1001
3,1,1002
4,2,2000
5,19,19000
6,20,20000
7,21,21000

# Map with div function touches two fields
Query::from("window")
    .map(Attribute("id") = Attribute("id") / Attribute("value"))
    .SINK;
----
5,1,1000
10,1,1001
15,1,1002
10,2,2000
1,19,19000
1,20,20000
1,21,21000

# Map with div function touches two fields and adds a new field
Query::from("window")
    .map(Attribute("new") = Attribute("id") / Attribute("value"))
    .SINK;
----
5,1,1000,5
10,1,1001,10
15,1,1002,15
20,2,2000,10
25,19,19000,1
30,20,20000,1
35,21,21000,1


# Two maps after each other that both touch the same fields
Query::from("window")
    .map(Attribute("id") = Attribute("id") / 5)
    .map(Attribute("value") = Attribute("value") / Attribute("id"))
    .SINK;
----
1,1,1000
2,0,1001
3,0,1002
4,0,2000
5,3,19000
6,3,20000
7,3,21000
