# name: function/arithmetical/FunctionExp.test
# description: Simple function exp tests
# groups: [Function, FunctionExp]

CREATE LOGICAL SOURCE stream(i8 INT8, i16 INT16, i32 INT32, i64 INT64, u8 UINT8, u16 UINT16, u32 UINT32, u64 UINT64, f32 FLOAT32, f64 FLOAT64);
CREATE PHYSICAL SOURCE FOR stream TYPE File;
ATTACH INLINE
0,1,2,3,0,1,2,3,0.5,1.5

CREATE SINK sinkStream(i8 FLOAT64, i16 FLOAT64, i32 FLOAT64, i64 FLOAT64, u8 FLOAT64, u16 FLOAT64, u32 FLOAT64, u64 FLOAT64, f32 FLOAT64, f64 FLOAT64) TYPE File;


# Use exp across different data types
SELECT
    EXP(i8) AS i8,
    EXP(i16) AS i16,
    EXP(i32) AS i32,
    EXP(i64) AS i64,

    EXP(u8) AS u8,
    EXP(u16) AS u16,
    EXP(u32) AS u32,
    EXP(u64) AS u64,

    EXP(f32) AS f32,
    EXP(f64) AS f64
FROM stream INTO sinkStream;
----
1.0,2.71828,7.38906,20.0855,1.0,2.71828,7.38906,20.0855,1.64872,4.48169
