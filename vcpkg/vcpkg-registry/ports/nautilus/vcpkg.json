{
  "name": "nautilus",
  "version-string": "latest",
  "description": "nautilus",
  "homepage": "https://github.com/nebulastream/nautilus",
  "supports": "x64 | (arm64 & !windows)",
  "features": {
    "mlir": {
      "dependencies": [
        {
          "name": "llvm",
          "default-features": false,
          "features": [
            "mlir",
            "target-x86",
            "target-aarch64"
          ]
        }
      ],
      "description": "Build with mlir support"
    },
    "debug": {
      "description": "Enable debug features including fork tracing, logging, and timing",
      "dependencies": [
        "cpptrace"
      ]
    }
  },
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "spdlog"
  ]
}
