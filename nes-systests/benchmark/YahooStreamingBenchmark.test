# name: milestone/YahooStreamingBenchmark.test
# description: YahooStreamingBenchmark queries (https://github.com/yahoo/streaming-benchmarks)
# groups: [milestone, benchmark, large]

# Source definitions
SourceCSV ysb1k UINT64 user_id UINT64 page_id UINT64 campaign_id UINT64 ad_type UINT64 event_type UINT64 current_ms UINT64 ip UINT64 d1 UINT64 d2 UINT32 d3 UINT16 d4 TESTDATA/ysb/ysb_1k_data.csv
SourceCSV ysb10k UINT64 user_id UINT64 page_id UINT64 campaign_id UINT64 ad_type UINT64 event_type UINT64 current_ms UINT64 ip UINT64 d1 UINT64 d2 UINT32 d3 UINT16 d4 TESTDATA/ysb/ysb_10k_data.csv

# Query 1 YSB with 1k campaign_ids
SELECT 
    campaign_id, 
    SUM(user_id) AS sum_user_id
FROM ysb1k
WHERE event_type < 1
GROUP BY campaign_id
WINDOW TUMBLING(current_ms, SIZE 30 SEC)
INTO CHECKSUM;
----
167167, 232106578


# Query 2 YSB with 10k campaign_ids
SELECT
    campaign_id,
    SUM(user_id) AS sum_user_id
FROM ysb10k
WHERE event_type < 1
GROUP BY campaign_id
WINDOW TUMBLING(current_ms, SIZE 30 SEC)
INTO CHECKSUM;
----
1517953,2109797738
