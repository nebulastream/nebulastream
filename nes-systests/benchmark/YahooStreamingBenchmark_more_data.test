# name: milestone/YahooStreamingBenchmark_more_data.test
# description: YahooStreamingBenchmark queries (https://github.com/yahoo/streaming-benchmarks)
# groups: [milestone, benchmark, large, Aggregation]

# Source definitions
CREATE LOGICAL SOURCE ysb1k(user_id VARSIZED NOT NULL, page_id VARSIZED NOT NULL, campaign_id VARSIZED NOT NULL, ad_id VARSIZED NOT NULL, ad_type VARSIZED NOT NULL, event_type VARSIZED NOT NULL, event_time UINT64 NOT NULL, ip_address VARSIZED NOT NULL);
CREATE PHYSICAL SOURCE FOR ysb1k TYPE File;
ATTACH FILE large/ysb/ysb1k_more_data_3GB.csv

CREATE LOGICAL SOURCE ysb10k(user_id VARSIZED NOT NULL, page_id VARSIZED NOT NULL, campaign_id VARSIZED NOT NULL, ad_id VARSIZED NOT NULL, ad_type VARSIZED NOT NULL, event_type VARSIZED NOT NULL, event_time UINT64 NOT NULL, ip_address VARSIZED NOT NULL);
CREATE PHYSICAL SOURCE FOR ysb10k TYPE File;
ATTACH FILE large/ysb/ysb10k_more_data_3GB.csv

CREATE SINK printSink(ysb1k.campaign UINT64 NOT NULL)  TYPE File;
CREATE SINK ysb1kChecksum(ysb1k.start UINT64 NOT NULL, ysb1k.end UINT64 NOT NULL, ysb1k.campaign_id VARSIZED NOT NULL, ysb1k.count_user_id UINT64 NOT NULL)  TYPE Checksum;
CREATE SINK ysb10kChecksum(ysb10k.start UINT64 NOT NULL, ysb10k.end UINT64 NOT NULL, ysb10k.campaign_id VARSIZED NOT NULL, ysb10k.count_user_id UINT64 NOT NULL)  TYPE Checksum;

# Adapt the YSB Query to already have the campaign_id, as we currently do not support stream-batch joins
# YSB Query
SELECT
    start,
    end,
    campaign_id,
    COUNT(user_id) AS count_user_id
FROM ysb1k
WHERE event_type == VARSIZED("view")
GROUP BY campaign_id
WINDOW TUMBLING(event_time, SIZE 30 SEC)
INTO ysb1kChecksum;
----
68000, 278998939

# Adapt the YSB Query to already have the campaign_id, as we currently do not support stream-batch joins
# YSB Query
SELECT
    start,
    end,
    campaign_id,
    COUNT(user_id) AS count_user_id
FROM ysb10k
WHERE event_type == VARSIZED("view")
GROUP BY campaign_id
WINDOW TUMBLING(event_time, SIZE 30 SEC)
INTO ysb10kChecksum;
