# Check if the values are divided correctly with a constant
SELECT
    # signed
    i8 / 42 AS i8,
    i16 / 42 AS i16,
    i32 / 42 AS i32,
    i64 / 42 AS i64,
    # unsigned
    u8 / 42 AS u8,
    u16 / 42 AS u16,
    u32 / 42 AS u32,
    u64 / 42 AS u64,
    # float
    f32 / 42.000000_F AS f32,
    f64 / 42.000000_D AS f64
FROM stream INTO sinkConstant
