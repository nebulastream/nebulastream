# Todo #442: the below system level tests currently perform downcasts.
# Check if different numeric types support the Less Equals operation
SELECT
    i8 <= 42 AS i8,
    i16 <= 42 AS i16,
    i32 <= 42 AS i32,
    i64 <= 42 AS i64,

    u8 <= 42_U AS u8,
    u16 <= 42_U AS u16,
    u32 <= 42_U AS u32,
    u64 <= 42_U AS u64,

    f32 <= 42.0_F AS f32,
    f64 <= 42.0_D AS f64
FROM stream INTO sinkConstant
----
1,1,1,1,1,0,0,0,1,0

# Less Equals i8 value with all other data types
SELECT
    # signed
    i8 <= i8 AS i8_i8,
    i8 <= i8 + 1 AS i8_i8_plus_1,
    i8 <= i16 AS i8_i16,
    i8 <= i16 + 1 AS i8_i16_plus_1,
    i8 <= i32 AS i8_i32,
    i8 <= i32 + 1 AS i8_i32_plus_1,
    i8 <= i64 AS i8_i64,
    i8 <= i64 + 1 AS i8_i64_plus_1,
    # unsigned
    i8 <= u8  AS i8_u8,
    i8 <= u8 + 1_UY AS i8_u8_plus_1,
    i8 <= u16 AS i8_u16,
    i8 <= u16 + 1_US AS i8_u16_plus_1,
    i8 <= u32 AS i8_u32,
    i8 <= u32 + 1_U AS i8_u32_plus_1,
    i8 <= u64 AS i8_u64,
    i8 <= u64 + 1_UL AS i8_u64_plus_1,
    # float
    i8 <= f32 AS i8_f32,
    i8 <= f32 + 1_F AS i8_f32_plus_1,
    i8 <= f64 AS i8_f64,
    i8 <= f64 + 1_D AS i8_f64_plus_1
FROM stream INTO sinkI8
