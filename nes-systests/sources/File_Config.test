# name: sources/FileConfig.test
# description: Forwarding queries with and without configurations
# groups: [Sources]

#==============================
#=====  NO Configuration  =====
#==============================

CREATE SINK noConfFileSink(noConfSourceFile.id UINT64 NOT NULL, noConfSourceFile.value UINT64 NOT NULL, noConfSourceFile.timestamp UINT64 NOT NULL)  TYPE File;
CREATE LOGICAL SOURCE noConfSourceFile(id UINT64 NOT NULL, value UINT64 NOT NULL, timestamp UINT64 NOT NULL);
CREATE PHYSICAL SOURCE FOR noConfSourceFile TYPE File SET('CSV' AS PARSER.`TYPE`);
ATTACH FILE small/stream8.csv

SELECT * FROM noConfSourceFile INTO noConfFileSink;
----
1,1,12
1,2,23
1,3,34
1,4,45
1,5,56

CREATE SINK noConfInlineSink(noConfSourceInline.id UINT64 NOT NULL, noConfSourceInline.value UINT64 NOT NULL, noConfSourceInline.timestamp UINT64 NOT NULL)  TYPE File;
CREATE LOGICAL SOURCE noConfSourceInline(id UINT64 NOT NULL, value UINT64 NOT NULL, timestamp UINT64 NOT NULL);
CREATE PHYSICAL SOURCE FOR noConfSourceInline TYPE File;
ATTACH INLINE
1,6,67
1,7,78
1,8,89
1,9,100

SELECT * FROM noConfSourceInline INTO noConfInlineSink;
----
1,6,67
1,7,78
1,8,89
1,9,100

