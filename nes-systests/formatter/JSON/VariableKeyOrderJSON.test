# name: JSON.test
# description: tests that the json input formatter supports different orderings for the keys
# groups: [Formatter, JSON]

CREATE LOGICAL SOURCE variableKeyOrderStream(key UINT64, value UINT64, name VARSIZED);
CREATE PHYSICAL SOURCE FOR variableKeyOrderStream TYPE File SET("JSON" AS PARSER.`TYPE`);
ATTACH INLINE
{"NAME":"john", "KEY":1, "VALUE":1}
{"VALUE":2, "KEY":1, "NAME":"max"}

CREATE SINK keySink(variableKeyOrderStream.key UINT64) TYPE File;
CREATE SINK valueSink(variableKeyOrderStream.value UINT64) TYPE File;
CREATE SINK nameSink(variableKeyOrderStream.name VARSIZED)  TYPE File;
CREATE SINK allFieldsSink(variableKeyOrderStream.key UINT64, variableKeyOrderStream.value UINT64, variableKeyOrderStream.name VARSIZED)  TYPE File;

SELECT key FROM variableKeyOrderStream INTO keySink;
----
1
1

SELECT value FROM variableKeyOrderStream INTO valueSink;
----
1
2

SELECT name FROM variableKeyOrderStream INTO nameSink;
----
john
max

SELECT * FROM variableKeyOrderStream INTO allFieldsSink;
----
1,1,john
1,2,max
