# name: guide/guide14.test
# description: Squared error using arithmetic expression
# groups: [Projection, Expression, Arithmetic]

CREATE LOGICAL SOURCE s(
    x INT32,
    y INT32
);

CREATE PHYSICAL SOURCE FOR s TYPE File;
ATTACH INLINE
5,3
2,7
10,10

CREATE SINK sink(
    `SQUARED_ERROR` INT32
) TYPE File;

SELECT
    (x - y) * (x - y) AS squared_error
FROM s
INTO sink;
