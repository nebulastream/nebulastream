SELECT SUM(value) as sumValue FROM (
    SELECT SUM(value) as value FROM stream WHERE id < UINT32(15) GROUP BY id WINDOW TUMBLING(timestamp, size 1 sec)
) WHERE id < UINT32(10) GROUP BY id WINDOW TUMBLING(start, size 2 sec) INTO sinkStream;
