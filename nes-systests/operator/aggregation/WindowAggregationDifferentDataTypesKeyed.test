CREATE SINK charKeySink(stream.start UINT64 NOT NULL, stream.end UINT64 NOT NULL, stream.c CHAR NOT NULL, stream.i16_Count
 UINT64 NOT NULL)  TYPE File;
# Char as aggregation key
SELECT start, end, c, COUNT(i16)
FROM stream GROUP BY (c) WINDOW SLIDING(ts, size 200 ms, advance by 100 ms) INTO charKeySink;
