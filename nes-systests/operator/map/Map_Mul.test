# name: map/Map_Add.test
# description: Simple map tests with a mul function
# groups: [Map]

CREATE LOGICAL SOURCE stream(id INT64, value INT64, timestamp INT64);
CREATE PHYSICAL SOURCE FOR stream TYPE File;
ATTACH INLINE
1,1,1000
2,1,1001
3,1,1002
4,2,2000
5,19,19000
6,20,20000
7,21,21000

CREATE SINK streamSink(id INT64, stream.value INT64, stream.timestamp INT64)  TYPE File;
CREATE SINK streamSinkWithNew(stream.id INT64, stream.value INT64, stream.timestamp INT64, new INT64)  TYPE File;
CREATE SINK streamSink3(id INT64, value INT64, stream.timestamp INT64)  TYPE File;


# Map with mul function touches one field
SELECT id * UINT32(3) AS id, value, timestamp FROM stream INTO streamSink;
----
3,1,1000
6,1,1001
9,1,1002
12,2,2000
15,19,19000
18,20,20000
21,21,21000

# Map with mul function touches two fields
SELECT id * value AS id, value, timestamp FROM stream INTO streamSink;
----
1,1,1000
2,1,1001
3,1,1002
8,2,2000
95,19,19000
120,20,20000
147,21,21000

# Map with mul function touches two fields and adds a new field
SELECT id, value, timestamp, id * value AS new FROM stream INTO streamSinkWithNew;
----
1,1,1000,1
2,1,1001,2
3,1,1002,3
4,2,2000,8
5,19,19000,95
6,20,20000,120
7,21,21000,147

# Two maps after each other that both touch the same fields without nesting
SELECT id * UINT32(3) AS id, id * value AS value, timestamp FROM stream INTO streamSink3;
----
3,1,1000
6,2,1001
9,3,1002
12,8,2000
15,95,19000
18,120,20000
21,147,21000

# Two maps after each other that both touch the same fields
SELECT id, id * value AS value, timestamp FROM (SELECT id * UINT32(3) AS id, value, timestamp FROM stream) INTO streamSink3;
----
3,3,1000
6,6,1001
9,9,1002
12,24,2000
15,285,19000
18,360,20000
21,441,21000
