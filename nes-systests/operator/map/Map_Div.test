# name: map/Map_Div.test
# description: Simple map tests with a div function
# groups: [Map]

CREATE LOGICAL SOURCE stream(id INT64 NOT NULL, value INT64 NOT NULL, timestamp INT64 NOT NULL);
CREATE PHYSICAL SOURCE FOR stream TYPE File;
ATTACH INLINE
5,1,1000
10,1,1001
15,1,1002
20,2,2000
25,19,19000
30,20,20000
35,21,21000

CREATE SINK streamSink(id INT64 NOT NULL, stream.value INT64 NOT NULL, stream.timestamp INT64 NOT NULL)  TYPE File;
CREATE SINK streamSink2(id INT64 NOT NULL, value INT64 NOT NULL, stream.timestamp INT64 NOT NULL)  TYPE File;
CREATE SINK streamSinkWithNew(stream.id INT64 NOT NULL, stream.value INT64 NOT NULL, stream.timestamp INT64 NOT NULL, new INT64 NOT NULL)  TYPE File;

# Map with div function touches one field
SELECT id / UINT32(5) AS id, value, timestamp FROM stream INTO streamSink;
----
1,1,1000
2,1,1001
3,1,1002
4,2,2000
5,19,19000
6,20,20000
7,21,21000

# Map with div function touches two fields
SELECT id / value AS id, value, timestamp FROM stream INTO streamSink;
----
5,1,1000
10,1,1001
15,1,1002
10,2,2000
1,19,19000
1,20,20000
1,21,21000

# Map with div function touches two fields and adds a new field
SELECT id, value, timestamp, id / value AS new FROM stream INTO streamSinkWithNew;
----
5,1,1000,5
10,1,1001,10
15,1,1002,15
20,2,2000,10
25,19,19000,1
30,20,20000,1
35,21,21000,1

# Two maps after each other that both touch the same fields
SELECT id, value / id AS value, timestamp FROM (
    SELECT value, timestamp, id / UINT32(5) AS id FROM stream
) INTO streamSink2;
----
1,1,1000
2,0,1001
3,0,1002
4,0,2000
5,3,19000
6,3,20000
7,3,21000
