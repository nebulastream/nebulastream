# Map with div function touches two fields
SELECT id / value AS id, value, timestamp FROM window INTO SINK
----
5,1,1000
10,1,1001
15,1,1002
10,2,2000
1,19,19000
1,20,20000
1,21,21000

# Map with div function touches two fields and adds a new field
SELECT id, value, timestamp, id / value AS new FROM window INTO SINK
----
5,1,1000,5
10,1,1001,10
15,1,1002,15
20,2,2000,10
25,19,19000,1
30,20,20000,1
35,21,21000,1

# Two maps after each other that both touch the same fields
SELECT id / 5 AS id, value / id AS value, timestamp FROM window INTO SINK
----
1,1,1000
2,0,1001
3,0,1002
4,0,2000
5,3,19000
6,3,20000
