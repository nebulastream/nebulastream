# name: function/arithmetical/FunctionRound.test
# description: Simple function round tests
# groups: [Function, FunctionRound]

CREATE LOGICAL SOURCE stream(i8 INT8, i16 INT16, i32 INT32, i64 INT64, u8 UINT8, u16 UINT16, u32 UINT32, u64 UINT64, f32 FLOAT32, f64 FLOAT64);
CREATE PHYSICAL SOURCE FOR stream TYPE File;
ATTACH INLINE
-42,-129,-32769,-2147483649,42,256,65536,4294967296,23.2,42.5

CREATE LOGICAL SOURCE stream2(f32 FLOAT32, f64 FLOAT64);
CREATE PHYSICAL SOURCE FOR stream2 TYPE File;
ATTACH INLINE
23.7,42.45
2.25,2.5
9.98,1.23

CREATE SINK sinkStream(i8 INT8, i16 INT16, i32 INT32, i64 INT64, u8 UINT8, u16 UINT16, u32 UINT32, u64 UINT64, f32 FLOAT32, f64 FLOAT64) TYPE File;
CREATE SINK sinkStream2(f32 FLOAT32, f64 FLOAT64) TYPE File;

# Use ROUND across different data types
SELECT
    ROUND(i8) AS i8,
    ROUND(i16) AS i16,
    ROUND(i32) AS i32,
    ROUND(i64) AS i64,

    ROUND(u8) AS u8,
    ROUND(u16) AS u16,
    ROUND(u32) AS u32,
    ROUND(u64) AS u64,

    ROUND(f32) AS f32,
    ROUND(f64) AS f64
FROM stream INTO sinkStream;
----
-42,-129,-32769,-2147483649,42,256,65536,4294967296,23,43

# Use ROUND for different floats
SELECT
    ROUND(f32) AS f32,
    ROUND(f64) AS f64
FROM stream2 INTO sinkStream2;
----
24,42
2,3
10,1
